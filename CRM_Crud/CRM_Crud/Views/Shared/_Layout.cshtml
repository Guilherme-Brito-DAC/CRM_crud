<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/css/reset.css">
    <link rel="stylesheet" href="~/css/index.css">
    <title>CRM</title>
</head>
<body>
    <div class="wrapper">
        <div class="top_navbar">
            <div class="hamburger">
                <div class="one"></div>
                <div class="two"></div>
                <div class="three"></div>
            </div>
            <div class="top_menu">
                <div class="logo">
                    CRM
                </div>
            </div>
        </div>

        <div class="sidebar">
            <ul>
                <li>
                    <a href="" class="active">
                        <img class="fas fa-book" src="https://img.icons8.com/ios-glyphs/20/ffffff/user--v1.png" alt="" />
                        <span class="title">Lead</span>
                    </a>
                </li>
                <li>
                    <a href="">
                        <img src="https://img.icons8.com/ios-glyphs/20/ffffff/courses.png" alt="" />
                        <span class="title">Curso</span>
                    </a>
                </li>
                <li>
                    <a href="">
                        <img src="https://img.icons8.com/ios-glyphs/20/ffffff/inscription.png" alt="" />
                        <span class="title">Inscrição</span>
                    </a>
                </li>

            </ul>
        </div>
        <div class="container">
            <main role="main" class="main_container">
                <div class="item">
                    Ops
                </div>
                @RenderBody()
            </main>
        </div>
    </div>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/navbar.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
