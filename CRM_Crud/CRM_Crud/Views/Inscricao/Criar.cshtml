@model CRM_Crud.Models.Inscricao

@{
    ViewData["Title"] = "Criar Inscrição";
}

<div class="item">
    <div class="tablehead">
        <h1 class="titulomid">Inscrição</h1>
        <a asp-action="Index">Voltar</a>
    </div>
    <hr />
    <div class="form-mid">
        <div class="">
            <form asp-action="View">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="nome" class="control-label"></label>
                    <input asp-for="nome" class="form-control" />
                    <span asp-validation-for="nome" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="curso_id" class="control-label"></label>
                    <select asp-for="curso_id" class="form-control">
                    </select>
                    <span asp-validation-for="curso_id" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="lead_id" class="control-label"></label>
                    <select asp-for="lead_id" class="form-control">
                    </select>
                    <span asp-validation-for="lead_id" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="data" class="control-label"></label>
                    <input asp-for="data" class="form-control" />
                    <span asp-validation-for="data" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="status" class="control-label"></label>
                    <select asp-for="status" class="form-control"></select>
                    <span asp-validation-for="status" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <input type="reset" value="Limpar" class="btn btn-secondary" />
                    <input type="submit" value="Cadastrar" class="btn btn-success" />
                </div>
            </form>
        </div>
    </div>
</div>


@section scripts {
<script src="~/js/site.js"></script>
<script src="~/lib/jquery/dist/jquery.js"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/js/navbar.js"></script>
}